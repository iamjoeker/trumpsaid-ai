{
  "name": "trumpsaidwtf-express",
  "license": "CC-BY-NC",
  "repository": {
    "type": "git",
    "url": ""
  },
  "scripts": {
    "start": "npm run serve",
    "build": "npm run build-sass && npm run build-sass-admin && npm run build-ts && npm run tslint && npm run copy-static-assets",
    "serve": "node dist/server.js",
    "watch-node": "nodemon dist/server.js",
    "watch": "concurrently -k -p \"[{name}]\" -n \"Sass,TypeScript,Node\" -c \"yellow.bold,cyan.bold,green.bold\" \"npm run watch-sass\" \"npm run watch-ts\"",
    "test": "jest --forceExit --coverage --verbose",
    "watch-test": "npm run test -- --watchAll",
    "build-watch": "npm run build && npm run watch",
    "build-ts": "tsc",
    "watch-ts": "tsc -w",
    "build-sass-admin": "node-sass src/public/css/admin.scss dist/public/css/admin.css",
    "build-sass": "node-sass src/public/css/main.scss dist/public/css/main.css",
    "watch-sass": "node-sass -w src/public/css/main.scss dist/public/css/main.css && node-sass -w src/public/css/admin.scss dist/public/css/admin.css",
    "tslint": "tslint -c tslint.json -p tsconfig.json",
    "copy-static-assets": "ts-node copyStaticAssets.ts",
    "debug": "npm run build && npm run watch-debug",
    "serve-debug": "nodemon --inspect dist/server.js",
    "watch-debug": "concurrently -k -p \"[{name}]\" -n \"Sass,TypeScript,Node\" -c \"yellow.bold,cyan.bold,green.bold\" \"npm run watch-sass\" \"npm run watch-ts\" \"npm run serve-debug\"",
    "serve-pm2": "pm2 start dist/server.js -n ts -a"
  },
  "dependencies": {
    "@types/dotenv": "^4.0.3",
    "@types/express-jwt": "0.0.40",
    "apollo-client": "^2.3.7",
    "apollo-client-preset": "^1.0.8",
    "apollo-server": "^2.0.0",
    "apollo-server-core": "^2.0.0",
    "apollo-server-errors": "^2.0.0",
    "apollo-server-express": "^2.0.0",
    "bcryptjs": "2.4.3",
    "body-parser": "^1.18.3",
    "compression": "^1.7.3",
    "connect-redis": "^3.3.3",
    "cors": "^2.8.4",
    "dotenv": "^6.0.0",
    "errorhandler": "^1.5.0",
    "express-flash": "0.0.2",
    "express-graphql": "^0.6.12",
    "express-jwt": "^5.3.1",
    "express-jwt-authz": "^1.0.0",
    "express-redis": "^2.0.0",
    "express-session": "^1.15.6",
    "express-validator": "^5.3.0",
    "graphql": "^0.13.2",
    "graphql-shield": "^2.2.6",
    "graphql-yoga": "1.14.12",
    "jsonwebtoken": "^8.3.0",
    "jwks-rsa": "^1.3.0",
    "lusca": "^1.6.0",
    "mdbootstrap": "^4.5.7",
    "passport": "^0.4.0",
    "passport-auth0": "^1.0.0",
    "prisma-binding": "2.1.1",
    "pug": "^2.0.3",
    "qs-middleware": "^1.0.3",
    "redis": "^2.8.0",
    "winston": "^3.0.0"
  },
  "devDependencies": {
    "@types/bcryptjs": "2.4.1",
    "@types/compression": "0.0.36",
    "@types/errorhandler": "0.0.32",
    "@types/express-graphql": "^0.6.1",
    "@types/express-session": "^1.15.10",
    "@types/graphql": "^0.13.3",
    "@types/jest": "^23.3.1",
    "@types/jsonwebtoken": "^7.2.8",
    "@types/lusca": "^1.5.0",
    "@types/passport": "^0.4.6",
    "@types/redis": "^2.8.6",
    "@types/shelljs": "^0.8.0",
    "@types/ws": "^5.1.2",
    "casual": "^1.5.19",
    "concurrently": "^3.6.1",
    "dotenv-cli": "1.4.0",
    "graphql-cli": "^2.16.5",
    "graphql-faker": "^1.7.5",
    "jest": "^23.4.2",
    "jest-cli": "^23.4.2",
    "node-sass": "^4.9.2",
    "nodemon": "1.18.3",
    "npm-run-all": "4.1.3",
    "prisma": "^1.12.0",
    "rimraf": "2.6.2",
    "shelljs": "^0.8.2",
    "ts-jest": "^23.0.1",
    "ts-loader": "^4.4.2",
    "ts-node": "6.2.0",
    "tslint": "^5.11.0",
    "tslint-config-airbnb": "^5.9.2",
    "typescript": "2.9.2",
    "webpack": "^4.16.3",
    "webpack-cli": "^3.1.0"
  }
}
